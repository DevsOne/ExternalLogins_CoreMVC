@model ExternalLogins_CoreMVC.Models.GoogleViewModel

<br />
<div class="form-horizontal">
    <p style="text-align:center;" class="text-info">
        You've successfully authenticated with <i><strong>@ViewBag.LoginProvider</strong></i>.<br />
        Please fill out the form below and click the Register button to finish
        logging in.
    </p>

    <div class="form-group">
        <label asp-for="ImageUrl" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <img src="@Model.ImageUrl" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Id" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Id" value="@Model.Id" readonly style="text-transform: capitalize;" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Name" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Name" value="@Model.Name" readonly style="text-transform: capitalize;" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="DisplayName" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="DisplayName" value="@Model.DisplayName" readonly style="text-transform: capitalize;" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="FirstName" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="FirstName" value="@Model.FirstName" readonly style="text-transform: capitalize;" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="LastName" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="LastName" value="@Model.LastName" readonly style="text-transform: capitalize;" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Email" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Email" value="@Model.Email" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Emails" class="col-md-2 control-label"></label>
        @foreach (var item in Model.Emails)
        {
            <div class="form-group" style="width:500px; float:left;">
                <div style="float:left;">
                    <label asp-for="@item.Email" class="col-md-2 control-label" style="width:150px;"></label>
                    <input type="text" value="@item.Email" readonly style="min-width:200px !important;; width:300px !important; float:right" />
                </div>
                <div style="float:left;">
                    <label asp-for="@item.EmailType" class="col-md-2 control-label" style="width:150px;"></label>
                    <input type="text" value="@item.EmailType" readonly style="min-width:200px !important; width:300px !important; float:right" />
                </div>
            </div>
        }
    </div>

    <div class="form-group">
        <label asp-for="Link" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Link" value="@Model.Link" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Kind" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Kind" value="@Model.Kind" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="ObjectType" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="ObjectType" value="@Model.ObjectType" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="IsPlusUser" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="IsPlusUser" value="@Model.IsPlusUser" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Language" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Language" value="@Model.Language" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="CircledByCount" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="CircledByCount" value="@Model.CircledByCount" readonly />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Verified" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input type="text" asp-for="Verified" value="@Model.Verified" readonly />
        </div>
    </div>

    @*<div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-default" value="Register" />
            </div>
        </div>*@
</div>